<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/ll_property"
    tools:context="com.yuyunchao.asus.myanimtest.PropertyAnimationActivity"
    android:fitsSystemWindows="true"
    android:clipToPadding="true">
    <Button
        android:id="@+id/btn_toxml"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start XML activity"

        />
    <Button
        android:id="@+id/btn_toview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start View activity"

        />
    <Button
        android:id="@+id/btn_tofishanim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start fishDrawable activity"
        />
    <Button
        android:id="@+id/btn_toTransitionanim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start TransitionAnim activity"
        />
</LinearLayout>
